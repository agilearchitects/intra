{
  "engines": {
    "node": "~12.8.0"
  },
  "version": "0.0.1",
  "scripts": {
    "build-api": "tsc --project src/api/tsconfig-publish.json",
    "watch-api": "tsc --watch",
    "serve-spa": "parcel src/spa/index.html --out-dir build/spa",
    "build-spa": "parcel build src/spa/index.html --out-dir dist/spa && OUT=dist yarn cp-spa",
    "cp-spa": "OUT=\"${OUT:-build}/spa/\" && mkdir -p $OUT && cp -r node_modules/tinymce/plugins $OUT && cp -r node_modules/tinymce/skins $OUT && cp -r node_modules/tinymce/themes $OUT",
    "watch-spa": "parcel watch src/spa/index.html --out-dir build/spa",
    "build-migrate": "tsc --project src/migrate/tsconfig-publish.json",
    "serve": "nodemon --inspect ./src/api/server.ts",
    "watch": "concurrently \"yarn run serve\" \"yarn run watch-api\" \"yarn run watch-spa\"",
    "build": "yarn build-api && yarn build-spa && yarn build-migrate",
    "test": "mocha --recursive src/**/*.spec.ts --require ts-node/register --timeout 999999",
    "lint": "tslint --config tslint.json --project tsconfig.json",
    "cli": "yarn ts-node src/cli/index.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn test ||Â yarn lint"
    }
  },
