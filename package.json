    "moment": "^2.24.0",
    "sqlite3": "^4.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typeorm": "^0.2.19",
    "winston": "^3.2.1"
  },
  "devDependencies": {
