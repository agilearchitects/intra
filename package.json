{
  "scripts": {
    "build-api": "tsc",
    "watch-api": "tsc --watch",
    "build-migrations": "tsc storage/migrations/*.ts -outDir ./build/migrations",
    "serve-spa": "parcel src/spa/index.html --out-dir build/spa",
    "build-spa": "parcel build src/spa/index.html --out-dir build/spa && yarn cp-spa",
    "cp-spa": "cp -r node_modules/tinymce/plugins build/spa/ && cp -r node_modules/tinymce/skins build/spa/ && cp -r node_modules/tinymce/themes build/spa/",
    "watch-spa": "parcel watch src/spa/index.html --out-dir build/spa",
    "serve": "nodemon ./build/server.js",
    "watch": "concurrently \"yarn run serve\" \"yarn run watch-api\" \"yarn run watch-spa\"",
    "build": "yarn build-api && yarn build-spa",
    "prepare": "yarn build",
    "render": "ts-node ./render.ts"
  },
  "dependencies": {
    "@agilearchitects/authenticaton": "^0.0.5",
